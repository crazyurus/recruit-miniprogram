<view class="page-content">
  <view class="content-block-title">面试经验分享</view>
  <view class="list-block media-list record-list">
    <navigator url="/pages/notice/detail?link={{item.link}}" wx:for="{{list}}" wx:key="link" class="item-link item-content">
      <view class="item-inner">
        <view class="item-title-row">
          <view class="item-title">
            <view class="circle bg-green"></view>
            <text>{{item.title}}</text>
          </view>
        </view>
        <view class="item-text">{{item.time}}</view>
      </view>
    </navigator>
  </view>
</view>